{
  "name": "@celo/rosettacli",
  "description": "CLI Tool for Rosetta",
  "version": "0.0.1-dev",
  "author": "Celo",
  "license": "Apache-2.0",
  "repository": "celo-org/rosetta",
  "homepage": "https://github.com/celo-org/rosetta",
  "bugs": "https://github.com/celo-org/rosetta/issues?utf8=%E2%9C%93&q=label%3Acli+",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "bin": {
    "rosettacli": "./bin/run"
  },
  "keywords": [
    "celo",
    "celocli",
    "celo-cli"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "clean": "tsc -b . --clean",
    "build": "tsc -b .",
    "docs": "yarn oclif-dev readme --multi --dir=../docs/command-line-interface && yarn prettier ../docs/command-line-interface/*.md --write",
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "prepack": "yarn run build && oclif-dev manifest && oclif-dev readme"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "cli-table": "^0.3.1",
    "cli-ux": "^5.3.1",
    "node-fetch": "^2.2.0",
    "web3": "1.2.4"
  },
  "devDependencies": {
    "@types/cli-table": "^0.3.0",
    "@types/fs-extra": "^8.0.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10",
    "@types/node-fetch": "^2.1.2",
    "globby": "^8",
    "prettier": "1.19.1",
    "typescript": "^3.7.3"
  },
  "files": [
    "README.md",
    "/bin",
    "/lib"
  ]
}
